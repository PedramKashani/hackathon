{
  "manifest_version": 3,
  "name": "Summary Extensions of the world!",
  "description": "Base Level Summary Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage", "nativeMessaging"],

  "backround": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
